{
  "name": "react-iiif-vault",
  "version": "1.5.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "web": "dist/bundle.global.js",
  "files": [
    "dist"
  ],
  "contributors": [
    "Stephen Fraser <stephen.fraser@digirati.com>"
  ],
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./canvas-panel": {
      "require": {
        "types": "./dist/canvas-panel.d.cts",
        "default": "./dist/canvas-panel.cjs"
      },
      "import": {
        "types": "./dist/canvas-panel.d.ts",
        "default": "./dist/canvas-panel.js"
      }
    },
    "./utils": {
      "require": {
        "types": "./dist/utils.d.cts",
        "default": "./dist/utils.cjs"
      },
      "import": {
        "types": "./dist/utils.d.ts",
        "default": "./dist/utils.js"
      }
    }
  },
  "scripts": {
    "build": "tsup && tsup --config tsup.umd.ts",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "prepack": "tsup && tsup --config tsup.umd.ts",
    "lint": "publint",
    "start": "vite"
  },
  "license": "MIT",
  "resolutions": {
    "react-reconciler": "0.29"
  },
  "dependencies": {
    "@atlas-viewer/atlas": "^2.2.9",
    "@iiif/helpers": "^1.3.0",
    "@iiif/parser": "^2.1.7",
    "@iiif/presentation-2": "^1.0.4",
    "@iiif/presentation-3": "^2.2.3",
    "@iiif/presentation-3-normalized": "^0.9.7",
    "mitt": "^3.0.1",
    "polygon-editor": "^0.0.4",
    "react-error-boundary": "^4.0.13",
    "react-lazy-load-image-component": "^1.6.0",
    "react-reconciler": "~0.29.0",
    "tiny-invariant": "^1.3.3",
    "zustand": "^4.5.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "^15.0.2",
    "@types/react": "^18.2.75",
    "@types/react-dom": "^18.2.24",
    "@types/react-lazy-load-image-component": "^1.6.3",
    "@vitejs/plugin-react": "^4.2.1",
    "esbuild-plugin-globals": "^0.2.0",
    "esbuild-plugin-umd-wrapper": "^2.0.0",
    "happy-dom": "^14.7.1",
    "prettier": "^3.2.5",
    "publint": "^0.2.8",
    "query-string": "^9.0.0",
    "tslib": "^2.6.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "vitest": "^1.6.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
